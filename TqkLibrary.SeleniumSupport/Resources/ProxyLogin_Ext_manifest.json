{
  "version": "1.0.0",
  "manifest_version": 2,
  "name": "ChromeProxy",
  "permissions": [
    "proxy",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "minimum_chrome_version": "22.0.0"
}